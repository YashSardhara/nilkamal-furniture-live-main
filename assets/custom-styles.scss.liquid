/** Variables **/
$light_blue: #0093DD;
$dark_blue: #121E6A;
$red: #E52F2F;

$w1024: '(max-width:1024px)';
$w991: '(max-width:991px)';
$w767: '(max-width:767px)';
$w480: '(max-width:480px)';
$w375: '(max-width:375px)';
$minw992: '(min-width:992px)';
$minw768: '(min-width:768px)';
$minw1025: '(min-width:1025px)';
$minw1200: '(min-width:1200px)';

$w1199: '(max-width:1199px)';
$w959: '(max-width:959px)';
$w798: '(max-width:798px)';
$minw799: '(min-width:799px)';
$minw960: '(min-width:960px)';
@font-face {
