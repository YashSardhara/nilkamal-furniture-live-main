    src: url('https://cdn.shopify.com/s/files/1/0248/7766/2271/files/GothamRounded-Book.woff2?v=1622536219') format('woff2'),
         url('https://cdn.shopify.com/s/files/1/0248/7766/2271/files/GothamRounded-Book.woff?v=1622536219') format('woff');
