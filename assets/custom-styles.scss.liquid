  @include justify-content(flex-end);
  width: 37%;
  padding-left: 2%;
  .nav-link {
    //margin-right: 9%;
    margin-right: 5%;
    &.wishlist {/*display: none;*/}
  }
  .menu.right {
    li {padding: 0;}
    a span {
      font-size: 14px;
      text-transform: none;
      padding: 0;
    }
  }
}
}
