  .all-products {
    background: #fff;
    width: 100%;
    margin: 30px 0;
    @include flexbox();
    height: 110px;
    // @include justify-content(center);
    @include align-items(center);
    @media(max-width:798px) {
      margin: 30px 0 10px;
    }
    @media(max-width:767px) {
      // flex-direction: column;
      height: auto !important;
      margin: 15px 0 20px;
      padding: 0;
    }
    .acontent img {
      width: 100%;
    }
    .heading {
      width: 200px;
      background: #f7f7f7;
      border-right: 0 solid #f7f5f3;
      margin: 0;
      padding: 10px 0;
      @include align-items(center);
      @include justify-content(center);
      @include flexbox();
      height: 100%;
      font-size: 16px;
      text-transform: capitalize;
      line-height: 1.2;
      color: #777;
      @media(max-width:767px) {
        width: 100%;
        height: auto;
        padding: 8px 0;
      }
    }
    .box {
      width: calc(100% - 200px);
      @media(max-width:767px) {
        width: 100%;
      }
      .slide {
        width: 20%;
        @media(max-width:767px) {
          width: 50%;
          padding-top: 10px;
          padding-bottom: 10px;
        }
        text-align: center;
        span {
          display: block;
          svg {
            width: 80px;
            height: 40px;
            * {
              stroke: rgb(147, 149, 152);
            }
             &:hover {
              * {
                stroke: #0093dd;
              }
            }
          }
        }
         &:hover {
          span {
            color: #0093dd;
          }
        }
      }
    }

    a,
    a:visited,
    a span {
      color: #777777;
    }
  }
  .paginate {
    padding: 0 15px;
    margin: 12px 0 50px;
    @media #{$w798} {
      margin-bottom: 25px;
    }
  }
}

/** Product deatil CSS starts here **/
div.breadcrumb-collection {
  display: none;
  .breadcrumb_text {
    display: none;
  }
  margin: 5px 0;
  font-family: 'Montserrat', sans-serif;
  span:last-child .breadcrumb_link span {
    color: $light_blue;
  }
  .breadcrumb_link {
    span {
      color: #A2A2A2;
      font-size: 12px;
      vertical-align: bottom;
      text-transform: none;
    }
  }
  .breadcrumb-divider {
    color: #A2A2A2;
    font-size: 12px;
  }
}
.product-template {
  .sixteen.columns {
    width: 100%;
  }
  @media #{$w798} {
    .breadcrumb-collection {
      display: none;
    }
    .columns {
      &.alpha {
        width: 100%;
      }
      &.omega {
        width: 100%;
      }
    }
  }
  @media #{$minw799} {
    .columns {
      &.alpha {
        width: 50%;
        padding-right: 3.1%;
        margin: 0;
        position: sticky;
        top: 0;
        bottom: 0;
        height: fit-content;
      }
      &.omega {
        width: 50%;
        margin: 0;
      }
    }
  }
  .gallery-wrap {
    .product_gallery_nav {
      // width: 87px;
      width: 90px;
      float: left;
      // margin-right: 24px;
      margin-right: 0;
      transform: rotate(90deg);
      .carousel-cell {
        width: 87px;
        height: 87px;
        border: 1px solid #B0D3E5;
        overflow: hidden;
        img {
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
        &.is-selected {
          border-color: $light_blue;
        }
      }
    }
    .product_gallery {
      width: calc(100% - 111px);
      float: right;
      .flickity-page-dots {
        bottom: 0;
        .dot {
          background: #0093dd;
        }
      }
    }
    @media #{$w798} {
      .product_gallery_nav {
        display: none;
      }
      .product_gallery {
        width: calc(100% + 30px);
        margin: 0 -15px 28px;
      }
    }
  }
  .alpha {
    .product_form {
      width: 100%;
      max-width: 100%;
      @media #{$w798} {
        margin: 0;
      }
    }
  }
  .purchase-details__buttons {
    width: 100%;
    float: right;
    @include justify-content(space-between);
    padding-bottom: 23px;
    position: relative;
    margin-top: 15px;
    .action_button,
    .shopify-payment-button {
      height: 46px;
      border-radius: 26px;
      font-weight: 700;
      width: calc(50% - 9px);
      margin-top: 0;
      @include flex(initial);
    }
    .add_to_cart {
      background: $dark_blue;
      border-color: $dark_blue;
    }
    .buy_now,
    .shopify-payment-button__button {
      background: $red;
      border-color: $red;
      font-size: 14px;
      font-weight: 400;
      margin-top: 0;
       &:hover {
        background: $red;
        border-color: $red;
      }
    }
    .items_left {
      color: $red;
      font-size: 12px;
      font-weight: 700;
      font-style: initial;
      text-align: center;
      width: calc(50% - 9px);
      margin: 0;
      position: absolute;
      bottom: 0;
      right: 0;
    }
    @media #{$w798} {
      width: 100%;
      padding: 0;
      position: fixed;
      bottom: 0;
      /*     left: 0; */
      z-index: 99;
      margin-left: -18px;
      .items_left {
        display: none;
      }
      .add_to_cart,
      .buy_now,
      .shopify-payment-button {
        width: 50%;
        height: 52px;
        border-radius: 0;
        .text {
          font-weight: 700;
        }
      }
      .shopify-payment-button__button {
        width: 100%;
        height: 52px;
        border-radius: 0;
        font-weight: 700;
      }
      .shopify-payment-button {
        background: #e52f2f;
        border-color: #e52f2f;
        width: 50%;
        display: none !important;
      }
      .product-template .purchase-details__buttons .custom_cart_action_button {
        background: #121e6a;
        border-color: #121e6a;
        border-radius: 0;
        width: 100%;
        height: 52px;
        border-radius: 0;
      }
    }
  }
  .product_name {
    color: #4D4D4D;
    text-transform: none;
    line-height: 1.4;
    padding: 0;
  }
  .wishlist-btn {
    display: none;
    margin-left: 23px;
    svg {
      fill: #7E7E7E;
    }
     &:hover {
      svg {
        fill: $light_blue;
      }
    }
  }
  .modal_price {
    padding: 0;
    margin-bottom: 0;
    @media #{$minw799} {
      @include flexbox();
      .price-info {
        // width: 58%;
        width: 100%;
      }
      .service-info {
        width: 42%;
        text-align: right;
        border-left: 1px solid #E1E1E1;
      }
    }
    .price-info {
      @include flexbox();
      @include flex-wrap(wrap);
      @include align-items(center);
    }
    .sale-text {
      width: 100%;
      font-weight: 700;
      text-transform: uppercase;
      color: #CC0C39;
      margin-top: 0;
      font-size: 12px;
      font-family: 'Poppins';
      line-height: 18px;
      margin-bottom: 2px;
    }
    .sale {
      color: #454C4C;
      font-size: 32px;
      font-weight: 500;
    }
    .was_price {
      color: #383838;
      margin: 0 5.5% 0 0;
      font-weight: bold;

    }
    .savings {
      @include savings_tag;
    }
    .service-info span {
      display: block;
      font-size: 12px;
      line-height: 1.2;
      &.seller {
        color: $light_blue;
      }
    }
    @media #{$w798} {
      .service-info {
        margin-top: 14px;
      }
    }
    @media #{$w480} {
      .sale {
        font-size: 18px;
      }
      .was_price {
        font-size: 13px;
        margin: 0 6% 0 2%;
      }
      .savings {
        font-size: 12px;
        padding: 0 7px 0 11px;
      }
    }
  }
  .form-box {
    @media #{$minw799} {
      @include flexbox();
      display: block;
      .left-col {
        width: 71%;
      }
      .right-col {
        width: 29%;
        margin: 0;
        display: contents;
      }
    }
    .left-col {
      max-width: 100%;
      table {
        margin-bottom: 0;
      }
    }
    .right-col {
      overflow: hidden;
      margin-top: 21px;
    }
    @media #{$w798} {
      ;
      // border-bottom: 1px solid #E1E1E1;
    }
  }
  .pincode-checker {
    // padding: 11px 21px 21px;
    padding-top: 20px;
    border: 1px solid #E0E6EF;
    label {
      color: $light_blue;
      font-weight: 400;
      text-transform: none;
    }
    input[type="text"].input-pincode {
      height: 40px;
      min-height: 40px;
      border-color: #BFBFBF;
      border-radius: 20px;
      padding: 0 126px 0 12px;
      margin-bottom: 0;
    }
    @include placeholder {
      color: #AFAFAF;
    }
    .action_button {
      width: 117px;
      height: 40px;
      min-height: 40px;
      position: absolute;
      right: 0;
      top: 0;
    }
    p {
      line-height: 16px;
      margin: 10px 0 0;
    }
  }
  .purchase-details {
    padding-bottom: 0;
    label {
      margin-bottom: 9px;
      margin-right: 24px;
      font-size: 14px;
    }
  }
  .swatch-element.color {
    min-width: 34px;
    min-height: 34px;
    .tooltip {
      /* visibility: visible; */
      background: none;
      color: #000;
      font-size: 12px;
      font-weight: 700;
      text-transform: uppercase;
      box-shadow: none;
      width: 100%;
      bottom: -22px;
      left: 0;
      padding: 0;
      margin: 0;
      transform: none;
       &:before,
       &:after {
        display: none;
      }
    }
    label {
      width: 26px;
      min-width: 26px;
      height: 26px !important;
      border: none;
    }
  }
  .social_buttons {
    border: none;
    padding: 15px 0 0;
    display: flex;
    // margin-left: 64px;
    margin-left: 0;
    margin-bottom: 6px;

    @media #{$w798} {
      margin-left: 0;
    }
    span.text {
      font-size: 14px;
      font-weight: 700;
      text-transform: uppercase;
      margin-right: 11px;
    }
    .share-btn {
      font-size: 12px;
      margin: 0 10px 0 0;
      a {
        width: 21px;
        height: 21px;
        padding: 0;
        border: none;
         &:before {
          color: #fff;
          margin: 5px 0 0;
        }
        &.icon-facebook-share {
          background: #124C87;
        }
        &.icon-twitter-share {
          background: #54A3F2;
        }
        &.icon-pinterest-share {
          background: #CB2027;
        }
      }
    }
    @media #{$w798} {
      text-align: left;
    }
  }
  .pdp.customer-services {
    padding: 18px 0 0;
    // border-top: 1px solid #E1E1E1;
    // border-bottom: 1px solid #E1E1E1;
    background-color: #f7f8fc;
    margin-bottom: 15px;
    margin-top: 60px;
    .container {
      padding: 0 !important;
      @include flexbox();
      @include flex-wrap(wrap);
    }
    .item {
      margin: 0 6% 18px 0;
      @include align-items(flex-start);
      span {
        span {
          margin: 0;
        }
        a {
          display: block;
          color: $light_blue;
          text-decoration: underline;
        }
      }
    }
    @media #{$w798} {
      margin-bottom: 20px;
      .item {
        // width: 50%;
        margin-right: 0;
        @include justify-content(center);
      }
    }
  }
  .product-description {
    .desc-container {}
    @media #{$minw799} {
      .desc-container {
         &:not(:last-child) {
          margin-bottom: 19px;
        }
      }
      h2 {
        pointer-events: none;
        span {
          display: none;
        }
      }
    }
    h2 {
      font-size: 20px;
      color: #4D4D4D;
      padding: 18px 31px 13px;
      margin: 0;
    }
    h3 {
      font-size: 16px;
      color: #000;
      margin-bottom: 4px;
    }
    p {
       &:last-child {
        margin: 0;
      }
    }
    table {
      margin: 0;
      tr {
        border-color: #E1E1E1;
         &:first-child {
          border-top: none;
        }
         &:last-child {
          border-bottom: none;
        }
      }
      td {
        border: none;
        padding: 5px 5px 5px 0;
         &:first-child {
          width: 47%;
        }
         &:last-child {
          width: 53%;
        }
      }
    }
    .desc-content {
      padding: 0 31px 18px;
      ul {
        padding-left: 14px;
        margin: 0;
        li {
          margin: 0;
           &:not(:last-child) {
            margin-bottom: 2px;
          }
        }
      }
    }
    .product-details {
      .desc-content {
        padding: 0;
        > div {
          padding: 13px 31px;
          & + div {
            border-top: 1px solid #E1E1E1;
          }
          @media #{$w798} {
            padding: 13px 18px;
          }
        }
      }
    }
    @media #{$w798} {
      .desc-container:not(:first-child) {
        border-top: 0;
      }
      h2 {
        font-size: 16px;
        padding: 13px 18px;
        span {
          @include transition(0.3s);
        }
        &.active {
          span {
            @include transform(rotateX(180deg));
          }
        }
      }
      h3 {
        font-size: 14px;
      }
      .desc-content {
        padding: 0 18px 18px;
      }
    }
  }
