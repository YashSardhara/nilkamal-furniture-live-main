/** Cart page CSS starts here **/
.cart-section {
  .page-header {
    h1 {
      color: #4D4D4D;
      font-weight: 400;
      padding: 0;
      margin: 0;
    }
    p {
      font-size: 16px;
      text-transform: uppercase;
      margin: 0;
    }
    .feature_divider {
      margin-top: 15px;
    }
  }
  h2 {
    color: #000;
    font-size: 20px;
  }
  .empty-cart {
    .continue-button {
      width: auto;
    }
  }
  .ten.columns,
  .four.columns {
    width: 100%;
  }
  @media #{$minw799} {
    .ten.columns {
      width: 64%;
      margin: 0;
    }
    .four.columns {
      width: 36%;
      margin: 0;
    }
    .cart_content_info {
      @include flexbox();
      @include flex-wrap(wrap);
      .left {
        width: 70%;
        padding-right: 15px;
      }
      .right {
        width: 30%;
        text-align: right;
      }
    }
  }
  .wrapper {
    background: #F9F9F9;
    padding: 20px 22px;
  }
  #cart_form {
    margin-bottom: 30px;
    .product-quantity-box {
      background: #FFF;
      width: 144px;
      height: 40px;
      margin: 0;
      .product-minus,
      .product-plus {
        line-height: 40px;
        height: 40px;
      }
      .quantity {
        width: 38%;
        height: 40px;
        font-size: 14px !important;
        color: #4D4D4D;
        font-family: "Nunito Sans", sans-serif;
      }
      @media #{$w798} {
        margin: 10px 0 15px;
      }
    }
  }
  .product {
    @include flexbox();
    & + .product {
      padding-top: 20px;
      border-top: 1px solid #E5E5E5;
      margin-top: 20px;
    }
    .alpha {
      width: 130px;
      overflow: hidden;
      .cart_page_image {
        display: block;
        width: 130px;
        height: 130px;
        overflow: hidden;
        img {
          width: 100%;
          height: 100%;
          object-fit: cover !important;
        }
      }
    }
    .cart_content_info {
      width: calc(100% - 130px);
      padding-left: 30px;
      [class^="icon-"]:before {
        font-size: 10px;
        font-weight: 700;
      }
    }
    h3 {
      line-height: 1.1;
      a {
        color: #4D4D4D;
        font-size: 16px;
        font-weight: 400;
        text-transform: none;
      }
    }
    .modal_price {
      color: #000;
      line-height: 1.2;
      padding: 0;
      margin: 0;
      .was_price {
        font-size: 16px;
        display: block;
      }
    }
    .remove-product {
      color: #000;
      font-size: 16px;
      top: -3px;
       &:before {
        margin-right: 11px;
      }
    }
    @media #{$w798} {
      .alpha {
        width: 40%;
        .cart_page_image {
          width: 100%;
          height: 100%;
          img {
            height: auto;
          }
        }
      }
      .cart_content_info {
        width: 60%;
        padding-left: 15px;
        > * {
          width: 100%;
        }
      }
      h3 {
        margin-bottom: 10px;
        a {
          font-size: 15px;
        }
      }
      .remove-product {
        font-size: 14px;
         &:before {
          margin-right: 8px;
        }
      }
      .modal_price {
        font-size: 18px;
        .was_price {
          font-size: 14px;
        }
      }
    }
  }
  .featured-links {
    margin-top: 21px;
  }
  .subtotal {
    padding: 22px;
    border: none;
