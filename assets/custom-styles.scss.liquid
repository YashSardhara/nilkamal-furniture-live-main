        width: 177px;
        .sort_by {
          border: 1px solid #D6D6D6;
        }
      }
      .sort_by {
        background-position-x: 96%;
        width: 100%;
        max-width: 100%;
        height: auto;
        min-height: auto;
        padding: 8px 30px 8px 10px;
        font-weight: 400;
        text-transform: capitalize;
      }
