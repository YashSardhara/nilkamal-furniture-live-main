/** Mixins **/
@mixin flexbox() {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
}
@mixin inline-flexbox() {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: -moz-inline-flex;
  display: -ms-inline-flexbox;
  display: inline-flex;
}
@mixin flex($values) {
  -webkit-box-flex: $values;
  -moz-box-flex: $values;
  -webkit-flex: $values;
  -ms-flex: $values;
  flex: $values;
}
@mixin flex-direction($direction) {
  -webkit-flex-direction: $direction;
  -moz-flex-direction: $direction;
  -ms-flex-direction: $direction;
  flex-direction: $direction;
}
@mixin flex-wrap($wrap) {
  -webkit-flex-wrap: $wrap;
  -moz-flex-wrap: $wrap;
  -ms-flex-wrap: $wrap;
  flex-wrap: $wrap;
}
@mixin flex-flow($flow) {
  -webkit-flex-flow: $flow;
  -moz-flex-flow: $flow;
  -ms-flex-flow: $flow;
  flex-flow: $flow;
}
@mixin order($val) {
  -webkit-box-ordinal-group: $val;
  -moz-box-ordinal-group: $val;
  -ms-flex-order: $val;
  -webkit-order: $val;
  order: $val;
}
@mixin flex-grow($grow) {
  -webkit-flex-grow: $grow;
  -moz-flex-grow: $grow;
  -ms-flex-grow: $grow;
  flex-grow: $grow;
}
@mixin flex-shrink($shrink) {
  -webkit-flex-shrink: $shrink;
  -moz-flex-shrink: $shrink;
  -ms-flex-shrink: $shrink;
  flex-shrink: $shrink;
}
@mixin flex-basis($width) {
  -webkit-flex-basis: $width;
  -moz-flex-basis: $width;
  -ms-flex-basis: $width;
  flex-basis: $width;
}
@mixin justify-content($justify) {
  -webkit-justify-content: $justify;
  -moz-justify-content: $justify;
  -ms-justify-content: $justify;
  justify-content: $justify;
  -ms-flex-pack: $justify;
}
@mixin align-content($align-content) {
  -webkit-align-content: $align-content;
  -moz-align-content: $align-content;
  -ms-align-content: $align-content;
  align-content: $align-content;
}
@mixin align-items($align-items) {
  -webkit-align-items: $align-items;
  -moz-align-items: $align-items;
  -ms-align-items: $align-items;
  align-items: $align-items;
}
@mixin align-self($align-self) {
  -webkit-align-self: $align-self;
  -moz-align-self: $align-self;
  -ms-align-self: $align-self;
  align-self: $align-self;
}
@mixin transition($duration) {
  -webkit-transition: all $duration;
  -moz-transition: all $duration;
  -ms-transition: all $duration;
  transition: all $duration;
}
@mixin transform($translate) {
  -webkit-transform: $translate;
  -moz-transform: $translate;
  -ms-transform: $translate;
  transform: $translate;
}
@mixin transform-origin($origin) {
  -webkit-transform-origin: $origin;
  -moz-transform-origin: $origin;
  -ms-transform-origin: $origin;
  transform-origin: $origin;
}
@mixin transformCenter {
  top: 50%;
  left: 50%;
  @include transform(translate(-50%, -50%));
}
@mixin transformXCenter {
  left: 50%;
  @include transform(translate(-50%, 0));
}
@mixin transformYCenter {
  top: 50%;
  @include transform(translate(0, -50%));
}
@mixin placeholder {
  ::-webkit-input-placeholder {@content}
  :-moz-placeholder           {@content}
  ::-moz-placeholder          {@content}
  :-ms-input-placeholder      {@content}
}
@mixin v_center_arrow {
  .flickity-prev-next-button {
    opacity: 1;
    background: #FFF;
    width: 35px;
    height: 35px;
    min-height: auto;
    padding: 0;
    border: 1px solid #7B7B7B;
    border-radius: 50%;
    @include transformYCenter;
    svg {
      width: 40%;
      height: 40%;
      padding: 0;
      left: 30%;
      .arrow {
        fill: #000;
      }
    }
    &.previous {left: -14px;}
    &.next {right: -14px;}
    @media #{$w798} {display: none;}
}
}
@mixin top_arrow {
  .flickity-prev-next-button {
    opacity: 1;
    background: #FFF;
    width: 51px;
    height: 51px;
    min-height: auto;
    padding: 0;
    border: 1px solid #707070;
    border-radius: 50%;
    svg {
      width: 40%;
      height: 40%;
      padding: 0;
      left: 30%;
      .arrow {
        fill: #000;
      }
    }
    &.previous {
      top: -80px;
      left: auto;
      right: 80px;
    }
    &.next {
      top: -80px;
      right: 15px;
    }
    @media #{$w798} {display: none;}
}
}
@mixin savings_tag {
  &:empty {display: none;}
  display: inline-block;
    position: relative;
    background: #CC0C39;
    color: #fff;
    font-family: Poppins;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    width: 75px;
    font-size: 12px;
    height: 25px;
    line-height: 25px;
    padding: 0 0px 0 0px;
    position: absolute;
    top: 14px;
    left: -2px;
    text-align: center;
    z-index: 1;
    border-right: 10px solid #FF003C;
}

/** Common CSS **/
.errors, .err, .form-error .disc {color: #ff0000;}
a {
  &:not(:hover) {color: #4d4d4d;}
  &:focus {outline: none;}
}
p:empty {display: none;}
.visible-desktop {
  @media #{$w798} {display: none !important;}
}
.visible-mobile {
  @media #{$minw799} {display: none !important;}
}

.main.content {
  min-height: 500px;
  padding: 0;
}

div.container {
  padding: 0 15px !important;
  margin-right: auto;
  margin-left: auto;
  // max-width: 1396px;
  max-width: 1330px;
}

input:not([type="checkbox"]):not([type='submit']):not([type="reset"]):not([type="button"]),
select, textarea {
  color: #4D4D4D;
  font-size: 12px;
  border-color: #CFCFCF;
  border-radius: 0;
  &:invalid {
    box-shadow: none;
  }
}
select {
  -webkit-appearance: none;
  -moz-appearance: none;
  cursor: pointer;
  padding-right: 40px;
}
textarea {
  height: 110px;
  resize: none;
}
@include placeholder {
  color: #8f8f8f;
  opacity: 1;
}
a.action_button, input.action_button, .action_button {
  font-size: 14px !important;
  font-weight: 400 !important;
  border-color: #DBDBDB;
  min-height: auto;
  padding: 0 28px;
   background: $light_blue;
    border-color: $light_blue;
  color: #fff;
  &:hover {
    background: $light_blue;
    border-color: $light_blue;
  }
  &.reverse {
    background: $light_blue;
    color: #fff;
    border-color: $light_blue;
    &:hover {
      background: #fff;
      border-color: #DBDBDB;
      color: #000;
    }
  }
}
.view_all {
  font-size: 14px;
  font-weight: 400;
  height: 40px;
  padding: 0 28px;
  line-height: 2.8;
}
.feature_divider {
  border-color: #E1E1E1;
}
.product-quantity-box {
  width: 5em;
  max-width: 100%;
  height: 40px;
  border: 1px solid #BFBFBF;
  border-radius: 26px;
  overflow: hidden;
  .quantity {
    height: 100%;
    min-height: auto;
    border: none;
    font-family: 'Montserrat', sans-serif;
    font-size: 20px;
  }
  input::-webkit-outer-spin-button,
  input::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }
  input[type=number] {
    -moz-appearance:textfield;
    font-size: 20px !important;
    &:focus {
      border: none;
    }
  }
  .product-plus, .product-minus {
    height: 100%;
    width: 31%;
    background: none;
    border: none;
    color: #707070;
    span {display: block;}
    &:hover {
      background: none;
    }
  }
  @media #{$w798} {
    width: 177px;
    height: 40px;
    .product-plus, .product-minus {
      line-height: 40px;
    }
    input.quantity {
      min-height: auto;
      font-size: 17px !important;
    }
  }
}
.customer-services {
  .customer-services-slider{
    width:100%;
    @media #{$minw1025} {
      pointer-events:none;
      .flickity-prev-next-button{
        display:none;
      }
    }
}
.item:first-child {
  svg{
    #Path_367{
      fill:#fff;
    }
  }
}
.item {
  position: relative;
  @include flexbox();
  @include align-items(center);
  svg {
    path {
      fill: $light_blue;
    }
  }
  span {
    line-height: 1.3;
    margin-left: 13px;
  }
}
.carousel:after {
  content: 'flickity';
  display: none; /* hide :after */
}
}
.product-wrap{
  border: 1px solid #E2E2E2;
  background: #fff;
  overflow: hidden;
  &:hover {
    .thumbnail-overlay {

    }
    .product-info__caption .shopify-product-form .add_to_cart.action_button{
      visibility:visible;
      opacity:1;
    }
  }
  .quick_shop {
    background: #fff;
    height: 38px;
    padding: 10px 25px;
    color: #4D4D4D;
    line-height: 1.2;

  }
  .product_image {
    height: 327px;
    a {
      display: block;
      height: 100%;
      .image__container {
        height: 100%;
        overflow: hidden;
      }
      img {
        height: 100%;
        object-fit: cover !important;
        &.secondary {
          position: absolute;
          top: 0;
        }
      }
    }
  }
  .product-info__caption {
    padding: 16px 25px;
    margin-top: 0 !important;
    position: static;
    .title {
      font-family: "Nunito Sans", sans-serif;
      color: #4D4D4D;
      font-weight: 400;
      text-transform: none;
      min-height: 33px;
      padding: 0;
      margin-bottom: 5px !important;
    }
    .price {
      padding: 0;
      margin-bottom: 12px !important;
      position: static;
      span.money {
        color: #000;
        font-size: 16px;
        font-weight: 700;
      }
      .sold_out {
        color: #FFF;
        font-family: 'Montserrat', sans-serif;
        font-size: 12px;
        font-weight: 400;
        text-transform: uppercase;
        background: #A8A8A8;
        display: block;
        width: 45px;
        height: 45px;
        border-radius: 50%;
        padding: 9px 0;
        position: absolute;
        top: 41px;
        left: 6px;
      }
    }
    .was_price {
      color: #B2B2B2;
      margin-left: 9px;
      span.money {
        color: #B2B2B2;
        font-size: 14px;
        font-weight: 400;
      }
    }
    .savings {
      @include savings_tag;
      font-size: 12px;
      height: 25px;
      line-height: 25px;
      padding-left: 9px;
      position: absolute;
      top: 4px;
      left: 20px;
      z-index: 1;
      &:before {
        width: 6px;
        height: 6px;
        top: 9px;
        left: -1px;
      }
      &:after {
        width: 19.25px;
        height: 19.25px;
        top: 3px;
        left: -7px;
      }
    }
    .product-collection-form{
      display:none;
    }
    .shopify-product-form {
      margin-bottom: 0;
      .purchase-details__buttons {padding: 0;}
      .add_to_cart {
        font-size: 12px !important;
        font-family: "Nunito Sans", sans-serif;
        background: $dark_blue;
        display: inline-block;
        width: auto;
        height: 38px;
        min-height: 38px;
        padding: 0 30px;
        border-color: $dark_blue;
        margin: 0 auto;
        opacity: 0;
        visibility: hidden;
        &.sold-out{
          background: gray;
          border-color: gray;
        }
        span {
          color: #fff;
          font-weight: 700;
        }
        @media #{$w798} {
          opacity: 1;
          visibility: visible;
        }
    }
  }
}
.wishlist-btn {
  width: 45px;
  height: 45px;
  border: 1px solid #EEEEEE;
  border-radius: 50%;
  line-height: 3.8;
  position: absolute;
  top: 5px;
  right: 9px;
  display: none;
  svg {fill: #929292;}
  &:hover svg {fill: $light_blue;}
}
}
.equal-specifier {
  margin: 0 -15px;
}
.products-slider {
  display: block !important;
  float: none;
  overflow: visible;
  @include v_center_arrow;
  &:hover {
    .flickity-prev-next-button {
      opacity: 1;
    }
  }
  .gallery-cell {
    width: calc((100% - 60px) / 4) !important;
    margin-right: 20px;
    @media #{$w1024} {
      width: calc((100% - 40px) / 3) !important;
    }
  @media #{$w798} {
    width: calc((100% - 20px) / 2) !important;
  }
@media #{$w480} {
  width: calc(80% - 20px) !important;
}
}
}
.paginate {
  .page a, .current {
    width: 35px;
    height: 35px;
    padding: 0;
    line-height: 35px;
  }
  .current {
    color: #FFF;
    background: $light_blue;
    border-color: $light_blue;
    font-weight: 400;
  }
  .page a {
    color: $light_blue;
    border-color: #B8B8B8;
  }
  .page a:hover, .current:hover {
    color: #FFF;
    background: $light_blue;
    border-color: $light_blue;
  }
  .prev, .next {
    a:hover {color: $light_blue;}
  }
  @media #{$w798} {
    .page a, .current {
      width: 30px;
      height: 30px;
      font-size: 12px;
      line-height: 28px;
    }
    .prev, .next {
      display: none;
    }
  }
}


/** Header CSS starts here **/
header {
  .promo_banner {
    min-height: 31px !important;
    font-size: 14px !important;
    font-family: "Nunito Sans", sans-serif;
    text-transform: none;
    padding: 8px 0;
    svg {
      fill: #FFF;
      vertical-align: middle;
      margin-left: 6px;
    }
    @media #{$w798} {
      padding: 6px 0;
      .promo_banner__content {
        font-size: 14px;
        padding: 0 10px;
      }
    }
}
.top_bar {
  height: auto;
  @include flexbox();
  @include flex-wrap(wrap);
  @include align-items(center);
  padding: 0;
}
.top-nav {
  display: block;
  width: 100%;
  ul.menu {
    list-style: none;
    padding: 0;
    margin: 0;
    text-align: right;
    li {
      display: inline-block;
      padding-left: 13px;
      margin: 7px 0;
      position: relative;
      & + li:before {
        content: "";
        display: block;
        width: 1px;
        height: 11px;
        background: #000;
        position: absolute;
        top: 14px;
        left: 0;
      }
      &:last-child {
        padding-right: 0;
      }
    }
    a {
      font-weight: 400;
      text-transform: capitalize;
          font-size: 15px;
    }
  }
}
div.logo.text-align--left {
  width: 184px;
  padding-left: 0;
}
.search_container {
  display: block;
  width: calc((100% - 185px) - 40%);
  padding-left: 2.5%;
  .search__results-wrapper{
    display: none !important;
  }
