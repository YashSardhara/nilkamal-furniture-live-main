    }
    .search-submit {
      background: $light_blue;
      width: 50px;
      height: 40px;
      top: 0;
      right: 0;
      border-top-right-radius: 6px;
      border-bottom-right-radius: 6px;
      text-align: center;
      &:before {
        color: #fff;
        font-size: 17px;
        line-height: 40px;
        margin: 0;
      }
