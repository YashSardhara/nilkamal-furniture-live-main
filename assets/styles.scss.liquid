}

.product-quantity-box {
  margin-right: 0;
  width: 45%;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  label {
    text-align: left;
  }
  .quantity {
    text-align: center;
    padding: 6px 15px;
    width: 38%;
    height: 44px;
    border-radius: 0;
    -webkit-appearance: none;
    float: left;
  }
  .product-plus,
  .product-minus {
    background: #f2f2f2;
    font-weight: 300;
    position: relative;
