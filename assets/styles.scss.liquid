    right: 0;
    bottom: 0;
    left: 0;
    opacity: 0;
    overflow: hidden;
    transition: opacity 0.3s ease-in;
    > a {
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
    }
    @include respond-to('medium') {
      display: none;
