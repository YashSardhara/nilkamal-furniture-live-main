.SideCartloader {
  border: 4px solid #f3f3f3; /* Loader border color */
  border-top: 4px solid #3498db; /* Loader spinner color */
  border-radius: 50%; /* Creates a circular loader */
  width: 30px; /* Loader width */
  height: 30px; /* Loader height */
  animation: AddTOspin .5s linear infinite; /* Spinner animation */
  margin: 50px auto; /* Center the loader */
}

/* Spinner animation */
@keyframes AddTOspin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

