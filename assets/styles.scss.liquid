   &:after,
   &:before {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 2;
  }
   &:after {
    width: 0;
    height: 0;
    border: 16px solid transparent;
    border-left: 20px solid #fff;
    margin-top: -18px;
    margin-left: -7px;
