<div class="footercont_main">
{% if template contains 'collection' %}
  {% include 'footer_content' %}
  {% include 'footer_content_1' %}
  {% include 'footer_content_2' %}
  {% include 'footer_content_3' %}
  {% include 'footer_content_4' %}
  {% include 'footer_content_5' %}
  {% include 'footer_content_6' %}
  {% include 'footer_content_7' %}
  
  
{% endif %}
</div>
<div class="footer-top">
  
  <div class="customer-services">
    <div class="container">
      <div class="customer-services-slider">
        <div class="item gallery-cell">
          {% include 'svg-icons', id:'easy_emi' %}
          <span>Easy EMI <br />Options</span>
        </div>
        <div class="item gallery-cell">
          {% include 'svg-icons', id:'free_delivery' %}
          <span>Free Home <br />Delivery*</span>
        </div>
        <div class="item gallery-cell">
          {% include 'svg-icons', id:'quality_assurance' %}
          <span>Quality <br />Assurance</span>
        </div>
        {% comment %}
        <div class="item gallery-cell">
          {% include 'svg-icons', id:'warranty' %}
          <span>1 Year <br />Warranty*</span>
        </div>
        {% endcomment %}
        {% if template != 'product' and template != 'collection' %}
        <div class="item gallery-cell">
          {% include 'svg-icons', id:'free_assembly' %}
          <span>Free <br />Assembly</span>
        </div>
        {% endif %}
      </div>
    </div>
  </div>
  
  <div class="newsletter">
    <div class="container center">
      <h4>Join our newsletter to receive updates on product annoucements and sales.</h4>
      <div class="newsletter-form clearfix">
        <span class="message"></span>
        {% if settings.mailchimp_link != blank %}
        <form action="{{ settings.mailchimp_link }}" method="post" name="mc-embedded-subscribe-form" id="contact_form" target="_blank">
          {% if block.settings.show_firstname %}
          <input type="text" value="" class="firstName" name="FNAME" placeholder="{{ 'general.newsletter_form.subscriber_first_name' | t }}">
          {% endif %}
          {% if block.settings.show_lastname %}
          <input type="text" value="" name="LNAME" placeholder="{{ 'general.newsletter_form.subscriber_last_name' | t }}">
          {% endif %}
          <input type="email" value="" name="EMAIL" class="contact_email" placeholder="{{ 'general.newsletter_form.placeholder' | t }}">
          <input type="submit" value="{{ 'general.newsletter_form.submit' | t }}" class="action_button sign_up" name="subscribe" />
        </form>
        {% else %}
        {% form 'customer' %}
        {% if form.posted_successfully? %}
        <p><em>{{ 'general.newsletter_form.success_text' | t }}</em></p>
        {% elsif form.errors %}
        {% for field in form.errors %}
        <p><em>{{ field }} - {{ form.errors.messages[field] }}</em></p>
        {% endfor %}
        {% endif %}

        <input type="hidden" name="contact[tags]" value="prospect,newsletter"/>
        {% if block.settings.show_firstname %}
        <input type="text" class="firstName" name="contact[first_name]" placeholder="{{ 'general.newsletter_form.subscriber_first_name' | t }}" />
        {% endif %}
        {% if block.settings.show_lastname %}
        <input type="text" name="contact[last_name]" placeholder="{{ 'general.newsletter_form.subscriber_last_name' | t }}" />
        {% endif %}

        <input type="hidden" name="challenge" value="false" />
        <input type="email" class="contact_email" name="contact[email]" required placeholder="{{ 'general.newsletter_form.placeholder' | t }}" />
        <input type='submit' class="action_button reverse sign_up" value="{{ 'general.newsletter_form.submit' | t }}" />
        {% endform %}
        {% endif %}
      </div>
    </div>
  </div>


  <div class="footer-content">
    <div class="container">
      <div class="content">
        {% if template contains "index" %}
        {% for block in section.blocks %}
        {% if block.type == "seoContent" %}
        {% if block.settings.seoTitle != '' %}
                  <h5>{{ block.settings.seoTitle }}</h5>
        {% endif %}

        {{ block.settings.seoDescription }}
        {% endif %}
        {% endfor %}
        {% else %}
        {% assign footerSeoTitle = collection.metafields.collection['Footer seo title'] %}
        {% assign footerSeoContent = collection.metafields.collection['Footer seo description'] %}
        {% if footerSeoTitle != 0 and footerSeoTitle != blank %}
        <h5>{{ footerSeoTitle }}</h5>
        {% endif %}
        {% if footerSeoContent != 0 and footerSeoContent != blank %}
        {{ footerSeoContent }} 
        {% endif %}
        {% endif %}
        {% if template contains "product" %}

        {% else %}

        {% if template contains "index" %}
        <div class="categories">
          <table>
            {% for block in section.blocks %}
            {% if block.type == "seoLinks" %}
            <tr>
              <td>
                <b>{{ block.settings.seoTitle }} : </b></td>
              <td>
                <p>
                  {%- assign popular_menu = block.settings.link -%}
                  {%- if linklists[popular_menu].links.size > 0 -%}
                  {%- for link in linklists[popular_menu].links -%}
                  <a href="{{ link.url}}">{{ link.title }},</a>{%- if forloop.last -%} {%- else -%}{%- endif -%}
                  {%- endfor -%}
                  {%- endif -%}
                </p>
              </td>
            </tr>
            {% endif %}
            {% endfor %}
          </table>
        </div>
        {% endif %}

        <!--       seo link title       -->
        {% assign seoLinkTitle1 = collection.metafields.collection['footer link title 1'] %}
        {% assign seoLinkTitle2 = collection.metafields.collection['footer link title 2'] %}
        {% assign seoLinkTitle3 = collection.metafields.collection['footer link title 3'] %}
        <!--       seo link menu       -->
        {% assign seoLinkMenu1 = collection.metafields.collection['footer link menu 1'] %}
        {% assign seoLinkMenu2 = collection.metafields.collection['footer link menu 2'] %}
        {% assign seoLinkMenu3 = collection.metafields.collection['footer link menu 3'] %}
        {% if seoLinkMenu1 != blank and seoLinkMenu2 != blank and seoLinkMenu3 != blank %}
        <div class="categories">
          <table>

            {% if template contains "index" %}

            {% else %}
            {% if seoLinkTitle1 != blank and seoLinkMenu1 != blank %}
            <tr>
              <td>
                <b>{{ seoLinkTitle1 }}</b>
              </td>
              <td>
                <p>
                  {%- assign popular_menu = seoLinkMenu1 -%}
                  {%- if linklists[popular_menu].links.size > 0 -%}
                  {%- for link in linklists[popular_menu].links -%}
                  <a href="{{ link.url}}">{{ link.title }}</a>{%- if forloop.last -%} {%- else -%},{%- endif -%}
                  {%- endfor -%}
                  {%- endif -%}
                </p>
              </td>
            </tr>
            {% endif %}
            {% if seoLinkTitle2 != blank and seoLinkMenu2 != blank %}
            <tr>
              <td>
                <b>{{ seoLinkTitle2 }}</b>
              </td>
              <td>
                <p>
                  {%- assign popular_menu = seoLinkMenu2 -%}
                  {%- if linklists[popular_menu].links.size > 0 -%}
                  {%- for link in linklists[popular_menu].links -%}
                  <a href="{{ link.url}}">{{ link.title }}</a>{%- if forloop.last -%} {%- else -%},{%- endif -%}
                  {%- endfor -%}
                  {%- endif -%}
                </p>
              </td>
            </tr>
            {% endif %}
            {% if seoLinkTitle3 != blank and seoLinkMenu3 != blank %}
            <tr>
              <td>
                <b>{{ seoLinkTitle3 }}</b>
              </td>
              <td>
                <p>
                  {%- assign popular_menu = seoLinkMenu3 -%}
                  {%- if linklists[popular_menu].links.size > 0 -%}
                  {%- for link in linklists[popular_menu].links -%}
                  <a href="{{ link.url}}">{{ link.title }}</a>{%- if forloop.last -%} {%- else -%},{%- endif -%}
                  {%- endfor -%}
                  {%- endif -%}
                </p>
              </td>
            </tr>
            {% endif %}
            {% endif %}
          </table>
        </div>
        {% endif %}
        {% endif %}
      </div>
      <span class="expand_content">{% include 'svg-icons', id:'down-arrow' %}</span>
    </div>
  </div>

</div>

