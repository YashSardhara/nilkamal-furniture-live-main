            {% if section.settings.logo_home != nil %}
            <img src="{{ section.settings.logo_home | img_url: '410x' }}" class="secondary_logo"
              alt="{{ shop.name | escape }}" />
            {% endif %}
            {% if section.settings.logo != nil %}
