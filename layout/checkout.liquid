<body>

  {{ skip_to_content_link }}

  <header class="banner" data-header role="banner">
    <div class="wrap">
      {{ content_for_logo }}
    </div>
  </header>
  {% comment %}
  <span class="super-sale-price-text">Use code <b>DEC10</b> to get extra 10%off on Best Deals Product</span>
  {% endcomment %}
  {{ order_summary_toggle }}
  <div class="content" data-content>
    <div class="wrap">
      <div class="main">
        <header class="main__header" role="banner">
          {{ content_for_logo }}
          {{ breadcrumb }}
          {{ alternative_payment_methods }}
        </header>
        <main class="main__content" role="main">
          {{ content_for_layout }}
        </main>
        <footer class="main__footer" role="contentinfo">
          {{ content_for_footer }}
        </footer>
      </div>
      <aside class="sidebar" role="complementary">
        <div class="sidebar__header">
          {{ content_for_logo }}
        </div>
        <div class="sidebar__content">
          {{ content_for_order_summary }}
          <!--       Sale       -->
          <div class="sidebar__header" style="display:block">
            {% comment %}
            Need for nilkamal sale shikha dont delete it
            {% if product.tags contains 'Best Deals' %}
            <div class="communications">
              <small id="p1">DEC10</small>
              <p id="to-copy"><b>DEC10</b> - Get extra 10% off on (Best Deals Product)
                <button id="applyBtn" onclick="copyToClipboard('#p1')" class="copyfrom">
                  APPLY
                </button>
              </p>
            </div>
            {% endif %}

            {% endcomment %}
          </div>

          <!--       End-Sale       -->


        </div>

      </aside>
    </div>
  </div>


  {{ tracking_code }}
  <style>
    .infomessage {
      padding-top: 20px;
      text-align: center;
      font-size: 16px;
      line-height: 1.8em;
      color: #0e649e;
      font-weight: bold;
    }

    .section--shipping-method {
      display: none;
    }

    iframe#preview-bar-iframe {
      display: none;
    }

    .super-sale-price-text {
      text-align: center;
      background-color: #0af;
      color: #fff;
      padding: 5px;
    }

    #continue_button:disabled {
      background: #dddddd;
    }

    #p1 {
      display: none
    }

    .communications {
      margin-top: -100px;
    }

    .offer-msg {
      position: relative;
      padding: 20px;
      border-radius: 10px;
      font-weight: 500;
      background: #fff;
      border: 2px solid #28b8c7;
      text-align: center;
      margin-bottom: 15px;
    }

    .offer-detail {
      margin-top: 5px;
    }

    p#to-copy {
      font-size: 18px;
    }

    small#p1 {
      display: none;
    }

    .copyfrom {
      width: 20px;
      margin-left: 10px;
    }

    .copyfrom {
      width: 90px !important;
      margin-left: 10px;
      background-color: #121e6a;
      padding: 10px 5px;
      border-radius: 5px;
      color: #fff;
      font-size: 14px;
    }

    .copyfrom:hover {
      background-color: #3c3c9f;
    }

    @media only screen and (max-width:768px) {
      .communications {
        margin-top: 15px;
      }
    }

  </style>
