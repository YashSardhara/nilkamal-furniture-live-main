                <label class="floating-label state_label_name pincode__checker_diasbled">State Name<span>*</span>
                </label>
                <span id="state_name_error" class="state_name_error err"></span>
              </div>
              <div class="fran_col floating-label-group">
                <input
                  type="text"
                  name="city_name"
                  id="city_name"
                  disabled
                  required>
                <label class="floating-label city_label_name pincode__checker_diasbled">City Name<span>*</span>
                </label>
                <span id="city_name_error" class="city_name_error err"></span>
                {% comment %} <ul class="city_sugg"></ul> {% endcomment %}
              </div>
            </div>
            <div class="fran_row">
              <div class="fran_col floating-label-group">
                <select
                  name="preferred_time_for_discussion"
                  id="preferred_time_for_discussion"
                  required>
                  <option value="Preferred time for discussion">Preferred time for discussion*</option>
                  <option value="Morning">Morning</option>
                  <option value="Afternoon">Afternoon</option>
                  <option value="Evening">Evening</option>
                </select>
              </div>
              <div class="fran_col floating-label-group">
                <input
                  type="text"
                  name="city"
                  id="contactFormcity_name"
                  autocorrect="off"
                  autocapitalize="off"
                  required>
                <label class="floating-label">Location Preference<span>*</span>
                </label>
              </div>

            </div>
            <div class="fran_row">
              <div class="fran_col form_col_rad ">
                <div class="fran_col_head">
                  <p>Do you own any franchisee</p>
                </div>
                <div class="fran_input">
                  <label class="form-check-label" for="radio1">
                    <input
                      type="radio"
                      class="form-check-input form__input_wid"
                      id="radio1"
                      name="franchisee"
                      onclick="radioEnable1()"
                      value="Yes"><span class="form_span">Yes</span>
                  </label>
                  <label class="form-check-label" for="radio2">
                    <input
                      type="radio"
                      class="form-check-input radio__input__disable1 form__input_wid"
                      id="radio2"
                      onclick="radioDisable1()"
                      name="franchisee"
                      value="No"
                      checked><span class="form_span">No</span>
                  </label>
                </div>
              </div>
              <div class="fran_col form_col_rad">
                <div class="fran_col_head">
                  <p>Do you own a retail space</p>
                </div>
                <div class="fran_input">
                  <label class="form-check-label" for="radio1">
                    <input
                      type="radio"
                      class="form-check-input form__input_wid"
                      id="radio3"
                      onclick="radioEnable2()"
                      name="reatil_space"
                      value="Yes"><span class="form_span">Yes</span>
                  </label>
                  <label class="form-check-label" for="radio2">
                    <input
                      type="radio"
                      class="form-check-input radio__input__disable2 form__input_wid"
                      id="radio4"
                      onclick="radioDisable2()"
                      checked
                      name="reatil_space"
                      value="No"><span class="form_span">No</span>
                  </label>
                </div>
              </div>

            </div>
            <div class="fran_row">
              <div class="fran_col floating-label-group">
                <select
                  name="property_availability"
                  id="property_availability"
                  disabled
                  required>
                  <option value="">Property Availability</option>
                  <option value="Available">Available</option>
                  <option value="Shortisted">Shortisted</option>
                  <option value="Will Search">Will Search</option>
                </select>

              </div>
              <div class="fran_col floating-label-group">
                <select
                  name="area_of_property_available"
                  id="area_of_property_available"
                  disabled
                  required>
                  <option value="">Area of property available</option>
                  <option value="Less than 8000 sq.ft.">Less than 8000 sq.ft.</option>
                  <option value="8000-10000 sq.ft.">8000-10000 sq.ft.</option>
                  <option value="above 10000 sq.ft.">above 10000 sq.ft.</option>
                </select>

              </div>
            </div>
            <div class="fran_row_full">
              <input
                type="submit"
                class="btn btn-Send"
                id="franchise_submit"
                value="Send Enquiry"
                required>
            </div>
          </form>
        </div>
      </div>
    </section>
  </main>
</section>
