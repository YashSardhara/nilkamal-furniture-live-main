  // Function to process CSV data and update city_name and state_name
  function processData(csv, pincode) {
    var lines = csv.split('\n');
    var headers = lines[0].split(',');
    var pincodeFound = false;
    for (var i = 1; i < lines.length; i++) {
      var currentLine = lines[i].split(',');
      var currentPincode = currentLine[0].trim();
      if (currentPincode === pincode) {
        var city = currentLine[1].trim();
      //  console.log("Testing The File city ", city)
        var stateName = currentLine[2].trim();
      //  console.log("Testing the file stet", stateName)
        pincodeFound = true;
        // You can break the loop if you only want the first match
        break;
      }
    }

    // Display error if pincode is not found
    if (!pincodeFound) {
      $("#pincode_name_error").html('Pincode does not exist');
      $('#city_name').val('');
      $('#state_name').val('');
