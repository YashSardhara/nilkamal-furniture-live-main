<style>
  .myLabelTop{
    top: -11px;
    bottom: 0;
    left: 5px;
    font-size: 11px;
    opacity: 1;
    color: #023e88;
    background: #fff;
    height: 20px;
    padding-inline: 10px;
    transition: .3s ease-in-out;
  }

  .pincode__checker_diasbled {
    color: rgba(0, 0, 0, .4) !important;
  }

  .noteContainer {
    display: flex;
    gap: 6px;
    align-items: center;
    margin-top: 5px;
  }

  .noteIcon svg {
    width: 16px;
    height: 16px;
    fill: #0093dd;
  }

  .noteText {
    display: flex;
    align-items: center;
  }

  .noteText span {
    color: #0093dd;
    margin-top: -0.5rem;
    font-weight: 400;
    line-height: normal;
    font-size: .8rem;
  }
</style>

