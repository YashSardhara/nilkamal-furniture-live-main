<script>
  var myID = document.getElementById("mybottomId1");

  var myScrollFunc = function () {
    var y = window.scrollY;
    if (y >= 2750) {
      myID.className = "bottom__bar__desktop show"
    } else {
      myID.className = "bottom__bar__desktop hide"
    }
  };

  window.addEventListener("scroll", myScrollFunc);

  var myID1 = document.getElementById("mybottomId2");

  var myScrollFunc1 = function () {
    var z = window.scrollY;
    if (z >= 1050) {
      myID1.className = "bottom__bar__desktop2 show"
    } else {
      myID1.className = "bottom__bar__desktop2 hide"
    }
  };

  window.addEventListener("scroll", myScrollFunc1);

  $('#contactFormFullName').keydown(function (e) {
    if (e.shiftKey || e.ctrlKey || e.altKey) {
      e.preventDefault();
    } else {
      var key = e.keyCode;
      if (!((key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
        e.preventDefault();
      }
    }
  });

  $(document).on('keypress', '#address_phone_no', function (e) {
    if ($(e.target).prop('value').length >= 10) {
      if (e.keyCode != 32) {
        return false
      }
    }
  })



  function radioEnable2() {
    document.getElementById("property_availability").disabled = false;
    document.getElementById("area_of_property_available").disabled = false;
    if ($("#radio_button").is(':checked')) {
      $("#property_availability").attr("required", "true");
      $("#area_of_property_available").attr("required", "true");
    }

  }

  function radioDisable2() {
    document.getElementById("property_availability").disabled = true;
    document.getElementById("area_of_property_available").disabled = true;
  }

  $('#alternate_address_phone_no').on('input', function () {
    $(this).val($(this).val().replace(/[^0-9]/g, '').substr(0, 10));
  });
</script>



