  $('#franchise_submit').click(function (e) {
    e.preventDefault();
    $('.err').html('');
    var fullname = $('#contactFormFullName').val();
    var email = $('#contactFormEmail').val();
    var phoneno = $('#address_phone_no').val();
    var pincode_name = $("#pincode_name").val();
    //var alternate_phone_no = $('#alternate_address_phone_no').val();
    var location_preference = $('#contactFormcity_name').val();
    var preferred_time_for_discussion = $('#preferred_time_for_discussion').val();
    var property_availability = $('#property_availability').val();
    var area_of_property_available = $('#area_of_property_available').val();
    {% comment %} var turnover = $('#alternate_address_phone_no').val(); {% endcomment %}
    var franchisee = $("input[name='franchisee']:checked").val();
    var state_name = $('#state_name').val();
    var reatil_space = $("input[name='reatil_space']:checked").val();
    var city_name = $('#city_name').val();
    var suggestion = $('#Suggestions').val();
    var urls = window.location.href;
    var parameter = urls.split('?');
    // console.log("_error",)
    $.ajax({
      url: "https://middleware.doctordreams.com/api/fofo-franchisee",
      type: 'POST',
      data: {
        "fullname": fullname,
        "email": email,
        "phoneno": phoneno,
        "pincode_name": pincode_name,
        "location_preference": location_preference,
        "preferred_time_for_discussion": preferred_time_for_discussion,
        "property_availability": property_availability,
        "area_of_property_available": area_of_property_available,

        // "turnover": turnover,
        "franchisee": franchisee,
        "state_name": state_name,
        "reatil_space": reatil_space,
        "city_name": city_name,
        "suggestion": suggestion,
        'parameter': parameter,
        'web_url': urls
      },
      dataType: 'json',
      beforeSend: function () {
        $('#franchise_submit').attr('disabled', true);
        $('#franchise_submit').html('Please wait...');
      },
      success: function (resp) {
        console.log(resp)
        $('#franchise').css('display', 'none');
        $('#thank_you').css('display', 'block');

        var url = "https://www.nilkamalfurniture.com/pages/franchise-enquiry-nilkamal-sleep";

        window.location = url;
      },
      error: function (data) {
        console.log('Vivek error data', data);
        $('#franchise_submit').attr('disabled', false);
        $('#franchise_submit').html('Send');
        var errr = data.responseJSON[0];

        console.log('parth error data', errr);
        $.each(errr, function (k, v) {
          $('.' + k + '_error').text(v).css('color', 'red');
        });
        document.getElementById("franchise_submit").style.marginTop = "20px";
        if (pincodeErrorDiv.text().trim() === "The pincode name must be 6 digits.") {
          // Change the content of the div to "Please enter valid Pincode"
          pincodeErrorDiv.text("Please enter valid Pincode");
        }
      }
    });
  });


