<script>
  // Function to get pincode data from CSV
  function getPincodeData() {
    var pincode = $('#pincode_name').val();
    var csvFile = 'https://cdn.shopify.com/s/files/1/0044/1208/0217/files/pincode_data.csv?v=1704458039';
    //console.log("This is csv file comment" , csvFile);
    $.ajax({
      type: 'GET',
      url: csvFile,
      dataType: 'text',
      success: function (data) {
        processData(data, pincode);
      }
    });
  }
  // Function to process CSV data and update city_name and state_name
  function processData(csv, pincode) {
    var lines = csv.split('\n');
    var headers = lines[0].split(',');
    var pincodeFound = false;
    for (var i = 1; i < lines.length; i++) {
      var currentLine = lines[i].split(',');
      var currentPincode = currentLine[0].trim();
      if (currentPincode === pincode) {
        var city = currentLine[1].trim();
        //  console.log("Testing The File city ", city)
        var stateName = currentLine[2].trim();
        //  console.log("Testing the file stet", stateName)
        pincodeFound = true;
        // You can break the loop if you only want the first match
        break;
      }
    }

    // Display error if pincode is not found
    if (!pincodeFound) {
      $("#pincode_name_error").html('Pincode does not exist');
      $("#pincode_name_error").css('color','red');
      $('#city_name').val('');
      $('#state_name').val('');
      $('#city_name').attr('disabled', true);
      $('#state_name').attr('disabled', true);
      $('.state_label_name').removeClass('myLabelTop');
      $('.city_label_name').removeClass('myLabelTop');
      $('#city_name').removeAttr('readonly');
      $('#state_name').removeAttr('readonly');
      $('.state_label_name').addClass('pincode__checker_diasbled');
      $('.city_label_name').addClass('pincode__checker_diasbled');
    } else {
      // Display data and show the "cityStateRow" class
      $("#pincode_name_error").html('');
      $('#city_name').removeAttr('disabled');
      $('#state_name').removeAttr('disabled');
      $('.state_label_name').addClass('myLabelTop');
      $('.city_label_name').addClass('myLabelTop');
      $('#city_name').attr('readonly', true);
      $('#state_name').attr('readonly', true);
      $('.state_label_name').removeClass('pincode__checker_diasbled');
      $('.city_label_name').removeClass('pincode__checker_diasbled');
      displayData(city, stateName);
    }
  }

  // Function to display data in city_name and state_name
  function displayData(city, stateName) {
    $('#city_name').val(city);
    $('#state_name').val(stateName);
    // console.log('City: ' + city);
    // console.log('State Name: ' + stateName);
  }

  // Function to handle focus out
  function handleFocusOut() {
    var pincode = $('#pincode_name').val().replace(/\D/g, '').substring(0, 6);

    if (pincode.length === 6) {
      // Assuming displayData is a function that takes city and stateName as parameters
      displayData(city, stateName);
    } else if (pincode.length < 6) {
      // If pincode has 5 digits, clear city_name and state_name
      $("#pincode_name_error").html('Please enter valid Pincode');
      $("#pincode_name_error").css('color','red');
      $('#noteContainer').css('display','none');
      $('#city_name').val('');
      $('#state_name').val('');
      $('#city_name').attr('disabled', true);
      $('#state_name').attr('disabled', true);
      $('.state_label_name').removeClass('myLabelTop');
      $('.city_label_name').removeClass('myLabelTop');
      $('#city_name').removeAttr('readonly');
      $('#state_name').removeAttr('readonly');
      $('.state_label_name').addClass('pincode__checker_diasbled');
      $('.city_label_name').addClass('pincode__checker_diasbled');
    }
  }

  // Event listener for input and focusout events
  $('#pincode_name').on('input', function (e) {
    var input = e.target.value;

    // Allow only numeric input
    var numericInput = input.replace(/\D/g, '');

    // Limit to 6 digits
    var pincode = numericInput.substring(0, 6);

    // Update the input value
    e.target.value = pincode;

    // Check if the entered pincode has 6 digits
    if (pincode.length === 6) {
      // Call your function when pincode is 6 digits
      getPincodeData();
    }
  });

  // Bind the handleFocusOut function to the focusout event
  $("#pincode_name").on('focusout', handleFocusOut);

  $(document).ready(function () {
    $('#thank_you').css('display', 'none');
