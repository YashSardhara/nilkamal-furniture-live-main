<script>
  // Function to get pincode data from CSV
  function getPincodeData() {
    var pincode = $('#trade_pincode-check').val();
    var csvFile = 'https://cdn.shopify.com/s/files/1/0044/1208/0217/files/pincode_data.csv?v=1704458039';
    $.ajax({
      type: 'GET',
      url: csvFile,
      dataType: 'text',
      success: function (data) {
        processData(data, pincode);
      }
    });
  }
  // Function to process CSV data and update trade_address_city and trade_address_state
  function processData(csv, pincode) {
    var lines = csv.split('\n');
    var headers = lines[0].split(',');
    var pincodeFound = false;
    for (var i = 1; i < lines.length; i++) {
      var currentLine = lines[i].split(',');
      var currentPincode = currentLine[0].trim();
      if (currentPincode === pincode) {
        var city = currentLine[1].trim();
        var stateName = currentLine[2].trim();
        pincodeFound = true;
        break;
      }
    }

    // Display error if pincode is not found
    if (!pincodeFound) {
      $("#trade_pincode_check_error").html('Pincode does not exist');
      $('#trade_address_city').val('');
      $('#trade_address_state').val('');
      $('#trade_address_city').attr('disabled', true);
      $('#trade_address_state').attr('disabled', true);
      $('#trade_address_city').removeAttr('readonly');
      $('#trade_address_state').removeAttr('readonly');
      $('.state__label').addClass('pincode__checker_diasbled');
      $('.city__label').addClass('pincode__checker_diasbled');
    } else {
      // Display data and show the "cityStateRow" class
      $("#trade_pincode_check_error").html('');
      $('#trade_address_city').removeAttr('disabled');
      $('#trade_address_state').removeAttr('disabled');
      $('#trade_address_city').attr('readonly', true);
      $('#trade_address_state').attr('readonly', true);
      $('.state__label').removeClass('pincode__checker_diasbled');
      $('.city__label').removeClass('pincode__checker_diasbled');
      displayData(city, stateName);
    }
  }

  // Function to display data in trade_address_city and trade_address_state
  function displayData(city, stateName) {
    $('#trade_address_city').val(city);
    $('#trade_address_state').val(stateName);
    console.log('City: ' + city);
    console.log('State Name: ' + stateName);
  }

  // Function to handle focus out
  function handleFocusOut() {
    var pincode = $('#trade_pincode-check').val().replace(/\D/g, '').substring(0, 6);

    if (pincode.length === 6) {
      displayData(city, stateName);
    } else if (pincode.length < 6) {
      $("#trade_pincode_check_error").html('Please enter valid Pincode');
      $('#trade_address_city').val('');
      $('#trade_address_state').val('');
      $('#trade_address_city').attr('disabled', true);
      $('#trade_address_state').attr('disabled', true);
      $('#trade_address_city').removeAttr('readonly');
      $('#trade_address_state').removeAttr('readonly');
      $('.state__label').addClass('pincode__checker_diasbled');
      $('.city__label').addClass('pincode__checker_diasbled');
    }
  }

  $('#trade_pincode-check').on('input', function (e) {
    var input = e.target.value;

    var numericInput = input.replace(/\D/g, '');

    var pincode = numericInput.substring(0, 6);

    e.target.value = pincode;

    if (pincode.length === 6) {
      getPincodeData();
    }
  });

  $("#trade_pincode-check").on('focusout', handleFocusOut);

  $('.generate_otp').click(function (e) {
    validateInputsOnsubmit();
    console.clear("console clear");
    console.log('e', e);
    var file_data = $('#warrantyImage').val();
    var MAX_FILE_SIZE = 1 * 1024 * 1024;
    var fileSize = $('#warrantyImage')[0]?.files[0]?.size;

    if (file_data != null || file_data != undefined) {
      var fileExt = file_data.substr(file_data.lastIndexOf('.') + 1).toLowerCase();
      if (fileExt === 'jpeg' || fileExt === 'png' || fileExt === 'jpg' || fileExt === 'pdf') {
        console.log('File is in proper format');
        if (fileSize < MAX_FILE_SIZE) {
          let input = $('#trade_pincode-check').val();
          let numericInput = input.replace(/\D/g, '');

          let pincode = numericInput.substring(0, 6);
          let myStateInput = $("#trade_address_state").val();  
        if (pincode.length === 6 && myStateInput != ''){
          var form = $('#trade_warrenty_form');
          $.ajax({
            url: "https://admin.nilkamalfurniture.com/api/trade-warranty-register",
            type: 'POST',
            data: form.serialize(),
            dataType: 'json',
            success: function (resp) {
              if (resp.success == true) {                             
                  if ($("#trade_online__tandc").is(":checked")) {
                    $('.warrenty-form').hide();
                    $('.form_head_p').hide();
                    $('.otp-form').show();
                  } else {
                    $('#trade_terms_cond_error').text('Please select terms and condition');
                  }
              }
            },
            error: function (data) {
              console.log('Errors Data Vivek', data.responseJSON[0])
              $.each(data.responseJSON[0], function (k, v) {
                $('#' + k + '_error').text(v);
              });
            },
          });
}
        }
        else {
          e.preventDefault();
          $('#warrantyImage').val("");
          warrantyfile_error.textContent = 'File size must be less than 1MB';
          file_upload_container.classList.add("warning");
        }
        warrantyfile_error.textContent = '';
        file_upload_container.classList.remove("warning");
      }
      else {
        warrantyfile_error_1.textContent = 'The selected file is not an image or a PDF';
        warrantyfile_error.textContent = '';
        console.log('Invalid file type in img');
      }
    }
    else {
      $('#warrantyImage').val("");
      warrantyfile_error_1.textContent = 'Please upload your Invoice document as image or a PDF';
      warrantyfile_error.textContent = '';
      console.log('File not uploaded');

      $('.file__name__message').css('display', 'none');
      $('.button__container').css('display', 'block');
    }

  });

  $('#online-purchase').change(function () {
    var online_val = $(this).val();
    if (online_val == 'onother') {
      $('#olothers').show();
    } else {
      $('#olothers').hide();
    }
  });
  $('#Offline-purchase').change(function () {
    var online_val = $(this).val();
    if (online_val == 'offother') {
      $('#ofothers').show();

    } else {
