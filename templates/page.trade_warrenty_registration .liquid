  });
  $('.verify_otp').click(function () {

    $("#trade_warrenty_form").ajaxSubmit({
      url: "https://admin.nilkamalfurniture.com/api/trade-verify-otp",
      type: 'POST',
      dataType: 'json',
      success: function (resp) {
   // alert("response");
        if (resp.success == true) {
          window.location.href = 'https://nilkamalfurniture.myshopify.com/pages/thank-you-page';
        } else {
          $('#otp_error').text('OTP not verified');
        }
      },
    });
  });
</script>
